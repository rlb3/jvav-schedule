<form {{action "create_schedule" on="submit"}}>
  <div class="form-group">
    <label for="date">Date</label>
    {{bs-input value=date id="date" placeholder="Date"}}
  </div>

  <div class="form-group">
    <label for="audio">Audio</label>
    {{bs-input value=audio id="audio" placeholder="Audio"}}

  </div>

  <div class="form-group">
    <label for="video">Video</label>
    {{bs-input value=video id="video" placeholder="Video"}}
  </div>

  <div class="form-group">
    <label for="alternate">Alternate</label>
    {{bs-input value=alternate id="alternate" placeholder="Alternate"}}
  </div>
  <button type="submit" class="btn btn-default btn-primary">Submit</button>
</form>
